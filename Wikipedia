        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# ---- STEP 4: Gemini wrapper using API key with explicit super().__init__() ----\\\\n\\\",\\n\",\n",
        "        \"        \\\"class GeminiLLM(LLM):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def __init__(self, temperature=0.7):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Call the parent constructor so that attributes are set properly\\\\n\\\",\\n\",\n",
        "        \"        \\\"        super().__init__()\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Now assign the temperature directly on self.__dict__\\\\n\\\",\\n\",\n",
        "        \"        \\\"        self.temperature = temperature\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # API endpoint based on available documentation (subject to change)\\\\n\\\",\\n\",\n",
        "        \"        \\\"        self.api_url = \\\\\\\"https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        self.api_key = os.environ.get(\\\\\\\"GEMINI_API_KEY\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if not self.api_key:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            raise ValueError(\\\\\\\"GEMINI_API_KEY not set. Please set it in the code.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def _call(self, prompt: str, stop=None):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        headers = {\\\\\\\"Content-Type\\\\\\\": \\\\\\\"application/json\\\\\\\"}\\\\n\\\",\\n\",\n",
        "        \"        \\\"        params = {\\\\\\\"key\\\\\\\": self.api_key}\\\\n\\\",\\n\",\n",
        "        \"        \\\"        body = {\\\\n\\\",\\n\",\n",
        "        \"        \\\"            \\\\\\\"contents\\\\\\\": [\\\\n\\\",\\n\",\n",
        "        \"        \\\"                {\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    \\\\\\\"parts\\\\\\\": [\\\\n\\\",\\n\",\n",
        "        \"        \\\"                        {\\\\\\\"text\\\\\\\": prompt}\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    ]\\\\n\\\",\\n\",\n",
        "        \"        \\\"                }\\\\n\\\",\\n\",\n",
        "        \"        \\\"            ],\\\\n\\\",\\n\",\n",
        "        \"        \\\"            \\\\\\\"generationConfig\\\\\\\": {\\\\\\\"temperature\\\\\\\": self.temperature}\\\\n\\\",\\n\",\n",
        "        \"        \\\"        }\\\\n\\\",\\n\",\n",
        "        \"        \\\"        try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            response = requests.post(self.api_url, headers=headers, params=params, json=body)\\\\n\\\",\\n\",\n",
        "        \"        \\\"            if response.status_code == 200:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                data = response.json()\\\\n\\\",\\n\",\n",
        "        \"        \\\"                return data[\\\\\\\"candidates\\\\\\\"][0][\\\\\\\"content\\\\\\\"][\\\\\\\"parts\\\\\\\"][0][\\\\\\\"text\\\\\\\"]\\\\n\\\",\\n\",\n",
        "        \"        \\\"            else:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                return f\\\\\\\"[Gemini API Error] {response.status_code}: {response.text}\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            return f\\\\\\\"[Gemini API Exception] {str(e)}\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    @property\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def _llm_type(self):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return \\\\\\\"gemini\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# ---- STEP 5: Helper functions for Wikipedia and FAISS ----\\\\n\\\",\\n\",\n",
        "        \"        \\\"def get_wikipedia_content(query, sentences=10):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        summary = wikipedia.summary(query, sentences=sentences, auto_suggest=True, redirect=True)\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return Document(page_content=summary, metadata={\\\\\\\"source\\\\\\\": \\\\\\\"Wikipedia\\\\\\\", \\\\\\\"topic\\\\\\\": query})\\\\n\\\",\\n\",\n",
        "        \"        \\\"    except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        print(f\\\\\\\"Error retrieving Wikipedia content for '{query}': {e}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return None\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_vectorstore(documents):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    embeddings = HuggingFaceEmbeddings(model_name=\\\\\\\"sentence-transformers/all-MiniLM-L6-v2\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return FAISS.from_documents(documents, embeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# ---- STEP 6: Chatbot logic ----\\\\n\\\",\\n\",\n",
        "        \"        \\\"llm = GeminiLLM()\\\\n\\\",\\n\",\n",
        "        \"        \\\"retriever = None\\\\n\\\",\\n\",\n",
        "        \"        \\\"qa_chain = None\\\\n\\\",\\n\",\n",
        "        \"        \\\"current_topic = \\\\\\\"\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def setup_chatbot(topic):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    global retriever, qa_chain, current_topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"    doc = get_wikipedia_content(topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if doc is None:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return f\\\\\\\"❌ No Wikipedia page found for '{topic}'.\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vectorstore = create_vectorstore([doc])\\\\n\\\",\\n\",\n",
        "        \"        \\\"    retriever = vectorstore.as_retriever(search_kwargs={\\\\\\\"k\\\\\\\": 1})\\\\n\\\",\\n\",\n",
        "        \"        \\\"    qa_chain = RetrievalQA.from_chain_type(llm=llm, retriever=retriever)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    current_topic = topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return f\\\\\\\"✅ Chatbot is ready! Ask anything about **{topic}**.\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def answer_question(user_question):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if qa_chain is None:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return \\\\\\\"⚠️ Please run setup_chatbot('your topic') first.\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return qa_chain.run(user_question)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# ---- STEP 7: Example usage ----\\\\n\\\",\\n\",\n",
        "        \"        \\\"print(setup_chatbot(\\\\\\\"Nikola Tesla\\\\\\\"))\\\\n\\\",\\n\",\n",
