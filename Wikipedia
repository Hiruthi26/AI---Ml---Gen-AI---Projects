        "        \"      ],\\n\",\n",
        "        \"      \\\"source\\\": [\\n\",\n",
        "        \"        \\\"# STEP 1: Install Required Packages\\\\n\\\",\\n\",\n",
        "        \"        \\\"!pip install -q langchain langchain-community langchain-google-genai google-generativeai faiss-cpu wikipedia sentence-transformers\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 2: Import Libraries\\\\n\\\",\\n\",\n",
        "        \"        \\\"import os\\\\n\\\",\\n\",\n",
        "        \"        \\\"import google.generativeai as genai\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain_google_genai import ChatGoogleGenerativeAI\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain_community.vectorstores import FAISS\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain_community.embeddings import HuggingFaceEmbeddings\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain_community.utilities import WikipediaAPIWrapper\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain.text_splitter import RecursiveCharacterTextSplitter\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain.chains import ConversationalRetrievalChain\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain.memory import ConversationBufferMemory\\\\n\\\",\\n\",\n",
        "        \"        \\\"from langchain.schema import SystemMessage, HumanMessage\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 3: Set your Google API Key (replace with your actual key)\\\\n\\\",\\n\",\n",
        "        \"        \\\"GOOGLE_API_KEY = \\\\\\\"AIzaSyCW5HzSyVUTqh7VbS6nFUiJsoo2fKSjbwM\\\\\\\"  # Replace with your actual API key\\\\n\\\",\\n\",\n",
        "        \"        \\\"os.environ[\\\\\\\"GOOGLE_API_KEY\\\\\\\"] = GOOGLE_API_KEY\\\\n\\\",\\n\",\n",
        "        \"        \\\"genai.configure(api_key=GOOGLE_API_KEY)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 4: Initialize Gemini LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"def get_llm():\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return ChatGoogleGenerativeAI(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model=\\\\\\\"gemini-1.5-pro\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        temperature=0.2,\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 5: Set up Wikipedia Content Retrieval and Processing\\\\n\\\",\\n\",\n",
        "        \"        \\\"def get_wiki_content(query, max_results=3):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Use the WikipediaAPIWrapper to get content\\\\n\\\",\\n\",\n",
        "        \"        \\\"    wiki = WikipediaAPIWrapper(top_k_results=max_results)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    wiki_content = wiki.run(query)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return wiki_content\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def process_text(text):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Split the text into chunks for better processing\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_splitter = RecursiveCharacterTextSplitter(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_size=1000,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_overlap=200,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        length_function=len\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"    chunks = text_splitter.split_text(text)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 6: Create FAISS Vector Database\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_vector_db(text_chunks):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Use Sentence Transformers for embeddings\\\\n\\\",\\n\",\n",
        "        \"        \\\"    embeddings = HuggingFaceEmbeddings(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_name=\\\\\\\"all-MiniLM-L6-v2\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_kwargs={'device': 'cpu'}\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create FAISS vector store from text chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = FAISS.from_texts(texts=text_chunks, embedding=embeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return vector_store\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 7: Create the Wikipedia Chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_wiki_chatbot(topic):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Getting Wikipedia information about: {topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Get content from Wikipedia\\\\n\\\",\\n\",\n",
        "        \"        \\\"    wiki_content = get_wiki_content(topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Process the text into chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_chunks = process_text(wiki_content)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Created {len(text_chunks)} text chunks\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create vector database\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = create_vector_db(text_chunks)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Created FAISS vector store\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Initialize the LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"    llm = get_llm()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Set up conversation memory\\\\n\\\",\\n\",\n",
        "        \"        \\\"    memory = ConversationBufferMemory(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory_key=\\\\\\\"chat_history\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_messages=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create conversational chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = ConversationalRetrievalChain.from_llm(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        llm=llm,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        retriever=vector_store.as_retriever(search_kwargs={\\\\\\\"k\\\\\\\": 3}),\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory=memory,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_source_documents=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return conversation_chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 8: Chat Function\\\\n\\\",\\n\",\n",
        "        \"        \\\"def chat_with_wiki(conversation_chain, query):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    response = conversation_chain({\\\\\\\"question\\\\\\\": query})\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return response[\\\\\\\"answer\\\\\\\"]\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 9: Main Chat Loop\\\\n\\\",\\n\",\n",
        "        \"        \\\"def run_wiki_chatbot(initial_topic=\\\\\\\"Artificial Intelligence\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create the chatbot with an initial topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = create_wiki_chatbot(initial_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"\\\\\\\\n===== Wikipedia Chatbot is ready! =====\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'new topic: [your topic]' to change topics\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'exit' to quit\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    while True:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        user_input = input(\\\\\\\"You: \\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower() == 'exit':\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Goodbye!\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            break\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Check if user wants to change the topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower().startswith(\\\\\\\"new topic:\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"            new_topic = user_input[len(\\\\\\\"new topic:\\\\\\\"):].strip()\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"\\\\\\\\nChanging topic to: {new_topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            conversation_chain = create_wiki_chatbot(new_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Topic changed. Ask a question about the new topic.\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            continue\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            # Get response from the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"            answer = chat_with_wiki(conversation_chain, user_input)\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"\\\\\\\\nWikipediaBot: {answer}\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"        except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"Error: {str(e)}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# STEP 10: Run the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"if __name__ == \\\\\\\"__main__\\\\\\\":\\\\n\\\",\\n\",\n",
        "        \"        \\\"    run_wiki_chatbot(\\\\\\\"Quantum Computing\\\\\\\")  # Start with this initial topic\\\"\\n\",\n",
        "        \"      ]\\n\",\n",
        "        \"    },\\n\",\n",
