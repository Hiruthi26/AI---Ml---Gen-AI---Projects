        "        \"        \\\"os.environ[\\\\\\\"GOOGLE_API_KEY\\\\\\\"] = GOOGLE_API_KEY\\\\n\\\",\\n\",\n",
        "        \"        \\\"genai.configure(api_key=GOOGLE_API_KEY)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Function to fetch Wikipedia data for a given topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"def fetch_wikipedia_content(query, num_results=3):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Searching Wikipedia for: {query}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Search for Wikipedia pages related to the query\\\\n\\\",\\n\",\n",
        "        \"        \\\"    search_results = wikipedia.search(query, results=num_results)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if not search_results:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return \\\\\\\"No Wikipedia results found for the query.\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    all_content = \\\\\\\"\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # For each search result, try to get the page content\\\\n\\\",\\n\",\n",
        "        \"        \\\"    for title in search_results:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            # Get the Wikipedia page\\\\n\\\",\\n\",\n",
        "        \"        \\\"            page = wikipedia.page(title, auto_suggest=False)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            # Add page title and content to our collection\\\\n\\\",\\n\",\n",
        "        \"        \\\"            all_content += f\\\\\\\"Title: {page.title}\\\\\\\\n\\\\\\\\nContent:\\\\\\\\n{page.content}\\\\\\\\n\\\\\\\\n\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"Added content from: {page.title}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"        except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"Error fetching {title}: {str(e)}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            continue\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return all_content\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Create embeddings using Sentence Transformers (alternative to GoogleGenerativeAIEmbeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_vector_db(text_chunks):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    embeddings = HuggingFaceEmbeddings(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_name=\\\\\\\"all-MiniLM-L6-v2\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_kwargs={'device': 'cpu'}\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create the FAISS vector store\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = FAISS.from_texts(texts=text_chunks, embedding=embeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return vector_store\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Text splitter function\\\\n\\\",\\n\",\n",
        "        \"        \\\"def split_text(text):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_splitter = RecursiveCharacterTextSplitter(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_size=1000,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_overlap=200,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        length_function=len\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"    chunks = text_splitter.split_text(text)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Set up the Gemini LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"def setup_gemini_llm():\\\\n\\\",\\n\",\n",
        "        \"        \\\"    llm = ChatGoogleGenerativeAI(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model=\\\\\\\"gemini-pro\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        temperature=0.2,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        top_p=0.85,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        top_k=40,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        max_output_tokens=2048,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        verbose=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return llm\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Create a Wikipedia chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_wikipedia_chatbot(topic):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 1. Fetch Wikipedia content\\\\n\\\",\\n\",\n",
        "        \"        \\\"    wiki_content = fetch_wikipedia_content(topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 2. Split the content into chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_chunks = split_text(wiki_content)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Created {len(text_chunks)} text chunks\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 3. Create vector store\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = create_vector_db(text_chunks)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Created FAISS vector store\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 4. Set up Gemini LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"    llm = setup_gemini_llm()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 5. Set up conversation memory\\\\n\\\",\\n\",\n",
        "        \"        \\\"    memory = ConversationBufferMemory(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory_key=\\\\\\\"chat_history\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_messages=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 6. Create conversational chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = ConversationalRetrievalChain.from_llm(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        llm=llm,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        retriever=vector_store.as_retriever(search_kwargs={\\\\\\\"k\\\\\\\": 4}),\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory=memory,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_source_documents=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return conversation_chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Function to interact with the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"def chat_with_wikipedia(conversation_chain, query):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    response = conversation_chain({\\\\\\\"question\\\\\\\": query})\\\\n\\\",\\n\",\n",
        "        \"        \\\"    answer = response[\\\\\\\"answer\\\\\\\"]\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Print source documents if available\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if \\\\\\\"source_documents\\\\\\\" in response:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        sources = response.get(\\\\\\\"source_documents\\\\\\\", [])\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if sources:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            sources_text = \\\\\\\"\\\\\\\\n\\\\\\\\nReferences:\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            for i, doc in enumerate(sources, 1):\\\\n\\\",\\n\",\n",
        "        \"        \\\"                sources_text += f\\\\\\\"\\\\\\\\n{i}. Content chunk from Wikipedia\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            answer += sources_text\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return answer\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Demo: Create a chatbot for a specific topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"def run_wikipedia_chatbot(initial_topic=\\\\\\\"Artificial Intelligence\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Initializing Wikipedia chatbot with topic: {initial_topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = create_wikipedia_chatbot(initial_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"\\\\\\\\n\\\\\\\" + \\\\\\\"=\\\\\\\"*50)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Wikipedia Chatbot is ready! Ask questions about the topic.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'new topic: [your topic]' to change the topic.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'exit' to end the conversation.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"=\\\\\\\"*50 + \\\\\\\"\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    while True:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        user_input = input(\\\\\\\"Your question: \\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower() == 'exit':\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Goodbye!\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            break\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Check if user wants to change the topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower().startswith(\\\\\\\"new topic:\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"            new_topic = user_input[len(\\\\\\\"new topic:\\\\\\\"):].strip()\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"\\\\\\\\nChanging topic to: {new_topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            conversation_chain = create_wikipedia_chatbot(new_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Topic changed successfully! Ask a question about the new topic.\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            continue\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Get response from the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"        response = chat_with_wikipedia(conversation_chain, user_input)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Display the response\\\\n\\\",\\n\",\n",
        "        \"        \\\"        display(Markdown(response))\\\\n\\\",\\n\",\n",
        "        \"        \\\"        print(\\\\\\\"\\\\\\\\n\\\\\\\" + \\\\\\\"-\\\\\\\"*50 + \\\\\\\"\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Run the chatbot - uncomment to run directly\\\\n\\\",\\n\",\n",
        "        \"        \\\"# run_wikipedia_chatbot(\\\\\\\"Quantum Computing\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Interactive UI with user input (better for Google Colab)\\\\n\\\",\\n\",\n",
        "        \"        \\\"from IPython.display import HTML, display\\\\n\\\",\\n\",\n",
        "        \"        \\\"import ipywidgets as widgets\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_ui():\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Initialize with AI topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = create_wikipedia_chatbot(\\\\\\\"Artificial Intelligence\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create output area\\\\n\\\",\\n\",\n",
        "        \"        \\\"    output = widgets.Output()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create text input\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_input = widgets.Text(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        value='',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        placeholder='Type your question or \\\\\\\"new topic: [topic]\\\\\\\" to change topics',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        description='',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        layout=widgets.Layout(width='80%')\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create send button\\\\n\\\",\\n\",\n",
        "        \"        \\\"    send_button = widgets.Button(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        description='Send',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        button_style='primary',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        layout=widgets.Layout(width='19%')\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create input area\\\\n\\\",\\n\",\n",
        "        \"        \\\"    input_area = widgets.HBox([text_input, send_button])\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Display widgets\\\\n\\\",\\n\",\n",
        "        \"        \\\"    display(widgets.VBox([output, input_area]))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    with output:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        display(Markdown(\\\\\\\"# Wikipedia Chatbot with Gemini\\\\\\\\n\\\\\\\\n**Current topic:** Artificial Intelligence\\\\\\\\n\\\\\\\\nAsk questions or type 'new topic: [your topic]' to change topics\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def on_button_click(b):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        query = text_input.value\\\\n\\\",\\n\",\n",
        "        \"        \\\"        text_input.value = ''\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        with output:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            display(Markdown(f\\\\\\\"**You:** {query}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            if query.lower().startswith(\\\\\\\"new topic:\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"                new_topic = query[len(\\\\\\\"new topic:\\\\\\\"):].strip()\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(f\\\\\\\"*Changing topic to: {new_topic}...*\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                nonlocal conversation_chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"                conversation_chain = create_wikipedia_chatbot(new_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(f\\\\\\\"**Topic changed to:** {new_topic}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"            else:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(\\\\\\\"*Thinking...*\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    response = chat_with_wikipedia(conversation_chain, query)\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    display(Markdown(f\\\\\\\"**Wikipedia Bot:** {response}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    display(Markdown(f\\\\\\\"**Error:** {str(e)}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            display(Markdown(\\\\\\\"---\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    send_button.on_click(on_button_click)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def on_enter(sender):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        on_button_click(None)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_input.on_submit(on_enter)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Uncomment to run the interactive UI\\\\n\\\",\\n\",\n",
        "        \"        \\\"# create_ui()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Example usage in Colab\\\\n\\\",\\n\",\n",
        "        \"        \\\"print(\\\\\\\"Run the following code to start the chatbot:\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"print(\\\\\\\"create_ui()\\\\\\\")\\\\\\\"  # Replace with your actual API key\\\\n\\\",\\n\",\n",
        "        \"        \\\"os.environ[\\\\\\\"GOOGLE_API_KEY\\\\\\\"] = GOOGLE_API_KEY\\\\n\\\",\\n\",\n",
        "        \"        \\\"genai.configure(api_key=GOOGLE_API_KEY)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Function to fetch Wikipedia data for a given topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"def fetch_wikipedia_content(query, num_results=3):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Searching Wikipedia for: {query}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Search for Wikipedia pages related to the query\\\\n\\\",\\n\",\n",
        "        \"        \\\"    search_results = wikipedia.search(query, results=num_results)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if not search_results:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return \\\\\\\"No Wikipedia results found for the query.\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    all_content = \\\\\\\"\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # For each search result, try to get the page content\\\\n\\\",\\n\",\n",
        "        \"        \\\"    for title in search_results:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            # Get the Wikipedia page\\\\n\\\",\\n\",\n",
        "        \"        \\\"            page = wikipedia.page(title, auto_suggest=False)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            # Add page title and content to our collection\\\\n\\\",\\n\",\n",
        "        \"        \\\"            all_content += f\\\\\\\"Title: {page.title}\\\\\\\\n\\\\\\\\nContent:\\\\\\\\n{page.content}\\\\\\\\n\\\\\\\\n\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"Added content from: {page.title}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"        except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"Error fetching {title}: {str(e)}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            continue\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return all_content\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Create embeddings using Sentence Transformers (alternative to GoogleGenerativeAIEmbeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_vector_db(text_chunks):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    embeddings = HuggingFaceEmbeddings(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_name=\\\\\\\"all-MiniLM-L6-v2\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model_kwargs={'device': 'cpu'}\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create the FAISS vector store\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = FAISS.from_texts(texts=text_chunks, embedding=embeddings)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return vector_store\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Text splitter function\\\\n\\\",\\n\",\n",
        "        \"        \\\"def split_text(text):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_splitter = RecursiveCharacterTextSplitter(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_size=1000,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        chunk_overlap=200,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        length_function=len\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"    chunks = text_splitter.split_text(text)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Set up the Gemini LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"def setup_gemini_llm():\\\\n\\\",\\n\",\n",
        "        \"        \\\"    llm = ChatGoogleGenerativeAI(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        model=\\\\\\\"gemini-1.5-pro\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        temperature=0.2,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        top_p=0.85,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        top_k=40,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        max_output_tokens=2048,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        verbose=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return llm\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Create a Wikipedia chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_wikipedia_chatbot(topic):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 1. Fetch Wikipedia content\\\\n\\\",\\n\",\n",
        "        \"        \\\"    wiki_content = fetch_wikipedia_content(topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 2. Split the content into chunks\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_chunks = split_text(wiki_content)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Created {len(text_chunks)} text chunks\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 3. Create vector store\\\\n\\\",\\n\",\n",
        "        \"        \\\"    vector_store = create_vector_db(text_chunks)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Created FAISS vector store\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 4. Set up Gemini LLM\\\\n\\\",\\n\",\n",
        "        \"        \\\"    llm = setup_gemini_llm()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 5. Set up conversation memory\\\\n\\\",\\n\",\n",
        "        \"        \\\"    memory = ConversationBufferMemory(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory_key=\\\\\\\"chat_history\\\\\\\",\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_messages=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # 6. Create conversational chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = ConversationalRetrievalChain.from_llm(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        llm=llm,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        retriever=vector_store.as_retriever(search_kwargs={\\\\\\\"k\\\\\\\": 4}),\\\\n\\\",\\n\",\n",
        "        \"        \\\"        memory=memory,\\\\n\\\",\\n\",\n",
        "        \"        \\\"        return_source_documents=True\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return conversation_chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Function to interact with the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"def chat_with_wikipedia(conversation_chain, query):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    response = conversation_chain({\\\\\\\"question\\\\\\\": query})\\\\n\\\",\\n\",\n",
        "        \"        \\\"    answer = response[\\\\\\\"answer\\\\\\\"]\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Print source documents if available\\\\n\\\",\\n\",\n",
        "        \"        \\\"    if \\\\\\\"source_documents\\\\\\\" in response:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        sources = response.get(\\\\\\\"source_documents\\\\\\\", [])\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if sources:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            sources_text = \\\\\\\"\\\\\\\\n\\\\\\\\nReferences:\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            for i, doc in enumerate(sources, 1):\\\\n\\\",\\n\",\n",
        "        \"        \\\"                sources_text += f\\\\\\\"\\\\\\\\n{i}. Content chunk from Wikipedia\\\\\\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            answer += sources_text\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    return answer\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Demo: Create a chatbot for a specific topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"def run_wikipedia_chatbot(initial_topic=\\\\\\\"Artificial Intelligence\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(f\\\\\\\"Initializing Wikipedia chatbot with topic: {initial_topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = create_wikipedia_chatbot(initial_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"\\\\\\\\n\\\\\\\" + \\\\\\\"=\\\\\\\"*50)\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Wikipedia Chatbot is ready! Ask questions about the topic.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'new topic: [your topic]' to change the topic.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"Type 'exit' to end the conversation.\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"    print(\\\\\\\"=\\\\\\\"*50 + \\\\\\\"\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    while True:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        user_input = input(\\\\\\\"Your question: \\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower() == 'exit':\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Goodbye!\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            break\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Check if user wants to change the topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"        if user_input.lower().startswith(\\\\\\\"new topic:\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"            new_topic = user_input[len(\\\\\\\"new topic:\\\\\\\"):].strip()\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(f\\\\\\\"\\\\\\\\nChanging topic to: {new_topic}\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            conversation_chain = create_wikipedia_chatbot(new_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"            print(\\\\\\\"Topic changed successfully! Ask a question about the new topic.\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"            continue\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Get response from the chatbot\\\\n\\\",\\n\",\n",
        "        \"        \\\"        response = chat_with_wikipedia(conversation_chain, user_input)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        # Display the response\\\\n\\\",\\n\",\n",
        "        \"        \\\"        display(Markdown(response))\\\\n\\\",\\n\",\n",
        "        \"        \\\"        print(\\\\\\\"\\\\\\\\n\\\\\\\" + \\\\\\\"-\\\\\\\"*50 + \\\\\\\"\\\\\\\\n\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Run the chatbot - uncomment to run directly\\\\n\\\",\\n\",\n",
        "        \"        \\\"# run_wikipedia_chatbot(\\\\\\\"Quantum Computing\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Interactive UI with user input (better for Google Colab)\\\\n\\\",\\n\",\n",
        "        \"        \\\"from IPython.display import HTML, display\\\\n\\\",\\n\",\n",
        "        \"        \\\"import ipywidgets as widgets\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"def create_ui():\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Initialize with AI topic\\\\n\\\",\\n\",\n",
        "        \"        \\\"    conversation_chain = create_wikipedia_chatbot(\\\\\\\"Artificial Intelligence\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create output area\\\\n\\\",\\n\",\n",
        "        \"        \\\"    output = widgets.Output()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create text input\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_input = widgets.Text(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        value='',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        placeholder='Type your question or \\\\\\\"new topic: [topic]\\\\\\\" to change topics',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        description='',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        layout=widgets.Layout(width='80%')\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create send button\\\\n\\\",\\n\",\n",
        "        \"        \\\"    send_button = widgets.Button(\\\\n\\\",\\n\",\n",
        "        \"        \\\"        description='Send',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        button_style='primary',\\\\n\\\",\\n\",\n",
        "        \"        \\\"        layout=widgets.Layout(width='19%')\\\\n\\\",\\n\",\n",
        "        \"        \\\"    )\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Create input area\\\\n\\\",\\n\",\n",
        "        \"        \\\"    input_area = widgets.HBox([text_input, send_button])\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    # Display widgets\\\\n\\\",\\n\",\n",
        "        \"        \\\"    display(widgets.VBox([output, input_area]))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    with output:\\\\n\\\",\\n\",\n",
        "        \"        \\\"        display(Markdown(\\\\\\\"# Wikipedia Chatbot with Gemini\\\\\\\\n\\\\\\\\n**Current topic:** Artificial Intelligence\\\\\\\\n\\\\\\\\nAsk questions or type 'new topic: [your topic]' to change topics\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def on_button_click(b):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        query = text_input.value\\\\n\\\",\\n\",\n",
        "        \"        \\\"        text_input.value = ''\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"        with output:\\\\n\\\",\\n\",\n",
        "        \"        \\\"            display(Markdown(f\\\\\\\"**You:** {query}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            if query.lower().startswith(\\\\\\\"new topic:\\\\\\\"):\\\\n\\\",\\n\",\n",
        "        \"        \\\"                new_topic = query[len(\\\\\\\"new topic:\\\\\\\"):].strip()\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(f\\\\\\\"*Changing topic to: {new_topic}...*\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                nonlocal conversation_chain\\\\n\\\",\\n\",\n",
        "        \"        \\\"                conversation_chain = create_wikipedia_chatbot(new_topic)\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(f\\\\\\\"**Topic changed to:** {new_topic}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"            else:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                display(Markdown(\\\\\\\"*Thinking...*\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                try:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    response = chat_with_wikipedia(conversation_chain, query)\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    display(Markdown(f\\\\\\\"**Wikipedia Bot:** {response}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"                except Exception as e:\\\\n\\\",\\n\",\n",
        "        \"        \\\"                    display(Markdown(f\\\\\\\"**Error:** {str(e)}\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"            display(Markdown(\\\\\\\"---\\\\\\\"))\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    send_button.on_click(on_button_click)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    def on_enter(sender):\\\\n\\\",\\n\",\n",
        "        \"        \\\"        on_button_click(None)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"    text_input.on_submit(on_enter)\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Uncomment to run the interactive UI\\\\n\\\",\\n\",\n",
        "        \"        \\\"# create_ui()\\\\n\\\",\\n\",\n",
        "        \"        \\\"\\\\n\\\",\\n\",\n",
        "        \"        \\\"# Example usage in Colab\\\\n\\\",\\n\",\n",
        "        \"        \\\"print(\\\\\\\"Run the following code to start the chatbot:\\\\\\\")\\\\n\\\",\\n\",\n",
        "        \"        \\\"print(\\\\\\\"create_ui()\\\\\\\")\\\"\\n\",\n",
        "        \"      ]\\n\",\n",
